<p id="notice"><%= notice %></p>

<h1>Listing Staffings</h1>

<table class='table table-hover'>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Middle name</th>
      <th>Cost per hour</th>
      <th>Lawfirm</th>
      <th>Matter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @staffings.each do |staffing| %>
      <tr>
        <td><%= staffing.first_name %></td>
        <td><%= staffing.last_name %></td>
        <td><%= staffing.middle_name %></td>
        <td><%= staffing.cost_per_hour %></td>
        <td><%= staffing.lawfirm %></td>
        <td><%= staffing.matter %></td>
        <td><%= link_to 'Show', staffing %></td>
        <td><%= link_to 'Edit', edit_staffing_path(staffing) %></td>
        <td><%= link_to 'Destroy', staffing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Staffing', new_staffing_path %>
